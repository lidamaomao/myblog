#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/webpack-dev-server@3.11.2_webpack@4.46.0/node_modules/webpack-dev-server/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/webpack-dev-server@3.11.2_webpack@4.46.0/node_modules/webpack-dev-server/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/webpack-dev-server@3.11.2_webpack@4.46.0/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vuepress/core@1.8.2/node_modules/@vuepress/core/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vuepress/core@1.8.2/node_modules/@vuepress/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vuepress/core@1.8.2/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vuepress/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../webpack-dev-server@3.11.2_webpack@4.46.0/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../../../../../../webpack-dev-server@3.11.2_webpack@4.46.0/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
