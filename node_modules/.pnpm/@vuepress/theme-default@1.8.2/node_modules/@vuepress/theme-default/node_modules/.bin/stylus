#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/stylus@0.54.8/node_modules/stylus/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/stylus@0.54.8/node_modules/stylus/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/stylus@0.54.8/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vuepress/theme-default@1.8.2/node_modules/@vuepress/theme-default/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vuepress/theme-default@1.8.2/node_modules/@vuepress/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vuepress/theme-default@1.8.2/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vuepress/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../stylus@0.54.8/node_modules/stylus/bin/stylus" "$@"
else
  exec node  "$basedir/../../../../../../../stylus@0.54.8/node_modules/stylus/bin/stylus" "$@"
fi
