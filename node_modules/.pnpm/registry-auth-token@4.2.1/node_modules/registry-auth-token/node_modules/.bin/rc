#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/rc@1.2.8/node_modules/rc/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/rc@1.2.8/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/registry-auth-token@4.2.1/node_modules/registry-auth-token/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/registry-auth-token@4.2.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
else
  exec node  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
fi
