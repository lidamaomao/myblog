#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/webpack@4.46.0/node_modules/webpack/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/webpack@4.46.0/node_modules/webpack/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/webpack@4.46.0/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/cache-loader@3.0.1_webpack@4.46.0/node_modules/cache-loader/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/cache-loader@3.0.1_webpack@4.46.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack@4.46.0/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../../../../../webpack@4.46.0/node_modules/webpack/bin/webpack.js" "$@"
fi
