#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/mkdirp@0.5.5/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/cache-loader@3.0.1_webpack@4.46.0/node_modules/cache-loader/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/cache-loader@3.0.1_webpack@4.46.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js" "$@"
fi
