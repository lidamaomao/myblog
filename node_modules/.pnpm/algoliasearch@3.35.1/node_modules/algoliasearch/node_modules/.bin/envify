#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/envify@4.1.0/node_modules/envify/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/envify@4.1.0/node_modules/envify/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/envify@4.1.0/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/algoliasearch@3.35.1/node_modules/algoliasearch/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/algoliasearch@3.35.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../envify@4.1.0/node_modules/envify/bin/envify" "$@"
else
  exec node  "$basedir/../../../../../envify@4.1.0/node_modules/envify/bin/envify" "$@"
fi
