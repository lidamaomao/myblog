#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/semver@5.7.1/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/algoliasearch@3.35.1/node_modules/algoliasearch/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/algoliasearch@3.35.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@5.7.1/node_modules/semver/bin/semver" "$@"
else
  exec node  "$basedir/../../../../../semver@5.7.1/node_modules/semver/bin/semver" "$@"
fi
