#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/@babel/parser@7.13.15/node_modules/@babel/parser/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@babel/parser@7.13.15/node_modules/@babel/parser/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@babel/parser@7.13.15/node_modules/@babel/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@babel/parser@7.13.15/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@babel/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@babel/core@7.13.15/node_modules/@babel/core/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@babel/core@7.13.15/node_modules/@babel/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@babel/core@7.13.15/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../parser@7.13.15/node_modules/@babel/parser/bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../../../../../../parser@7.13.15/node_modules/@babel/parser/bin/babel-parser.js" "$@"
fi
