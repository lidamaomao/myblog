#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/browserslist@4.16.3/node_modules/browserslist/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/browserslist@4.16.3/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@babel/helper-compilation-targets@7.13.13_@babel+core@7.13.15/node_modules/@babel/helper-compilation-targets/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@babel/helper-compilation-targets@7.13.13_@babel+core@7.13.15/node_modules/@babel/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@babel/helper-compilation-targets@7.13.13_@babel+core@7.13.15/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@babel/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../browserslist@4.16.3/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../../browserslist@4.16.3/node_modules/browserslist/cli.js" "$@"
fi
