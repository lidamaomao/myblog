#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/vuepress@1.8.2/node_modules/vuepress/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/vuepress@1.8.2/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vssue/vuepress-plugin-vssue@1.4.7_vuepress@1.8.2/node_modules/@vssue/vuepress-plugin-vssue/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vssue/vuepress-plugin-vssue@1.4.7_vuepress@1.8.2/node_modules/@vssue/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vssue/vuepress-plugin-vssue@1.4.7_vuepress@1.8.2/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vssue/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../vuepress@1.8.2/node_modules/vuepress/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../../vuepress@1.8.2/node_modules/vuepress/cli.js" "$@"
fi
