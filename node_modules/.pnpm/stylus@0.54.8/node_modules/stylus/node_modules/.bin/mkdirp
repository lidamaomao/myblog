#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/mkdirp@1.0.4/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/stylus@0.54.8/node_modules/stylus/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/stylus@0.54.8/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
fi
