#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/nopt@1.0.10/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules/nopt/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules/nopt/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nopt/bin/nopt.js" "$@"
else
  exec node  "$basedir/../nopt/bin/nopt.js" "$@"
fi
