#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/multicast-dns@6.2.3/node_modules/multicast-dns/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/multicast-dns@6.2.3/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules/multicast-dns/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../multicast-dns/cli.js" "$@"
else
  exec node  "$basedir/../multicast-dns/cli.js" "$@"
fi
