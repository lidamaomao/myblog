#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/cssesc@3.0.0/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules/cssesc/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules/cssesc/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cssesc/bin/cssesc" "$@"
else
  exec node  "$basedir/../cssesc/bin/cssesc" "$@"
fi
