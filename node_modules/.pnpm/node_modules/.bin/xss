#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/xss@1.0.8/node_modules/xss/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/xss@1.0.8/node_modules/xss/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/xss@1.0.8/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules/xss/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules/xss/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../xss/bin/xss" "$@"
else
  exec node  "$basedir/../xss/bin/xss" "$@"
fi
