#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/vuepress-plugin-sitemap@2.3.1/node_modules/vuepress-plugin-sitemap/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/vuepress-plugin-sitemap@2.3.1/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules/vuepress-plugin-sitemap/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vuepress-plugin-sitemap/cli.js" "$@"
else
  exec node  "$basedir/../vuepress-plugin-sitemap/cli.js" "$@"
fi
