#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/markdown-it@8.4.2/node_modules/markdown-it/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/markdown-it@8.4.2/node_modules/markdown-it/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/markdown-it@8.4.2/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules/markdown-it/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules/markdown-it/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../markdown-it/bin/markdown-it.js" "$@"
else
  exec node  "$basedir/../markdown-it/bin/markdown-it.js" "$@"
fi
