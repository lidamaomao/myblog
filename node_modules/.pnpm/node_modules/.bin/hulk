#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/hogan.js@3.0.2/node_modules/hogan.js/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/hogan.js@3.0.2/node_modules/hogan.js/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/hogan.js@3.0.2/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules/hogan.js/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules/hogan.js/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../hogan.js/bin/hulk" "$@"
else
  exec node  "$basedir/../hogan.js/bin/hulk" "$@"
fi
