#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/window-size@0.1.4/node_modules/window-size/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/window-size@0.1.4/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/yargs@3.32.0/node_modules/yargs/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/yargs@3.32.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../window-size@0.1.4/node_modules/window-size/cli.js" "$@"
else
  exec node  "$basedir/../../../../../window-size@0.1.4/node_modules/window-size/cli.js" "$@"
fi
