#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/js-yaml@3.14.1/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/cosmiconfig@5.2.1/node_modules/cosmiconfig/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/cosmiconfig@5.2.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../js-yaml@3.14.1/node_modules/js-yaml/bin/js-yaml.js" "$@"
else
  exec node  "$basedir/../../../../../js-yaml@3.14.1/node_modules/js-yaml/bin/js-yaml.js" "$@"
fi
