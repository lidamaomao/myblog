#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/terser@4.8.0/node_modules/terser/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/terser@4.8.0/node_modules/terser/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/terser@4.8.0/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/terser-webpack-plugin@1.4.5_webpack@4.46.0/node_modules/terser-webpack-plugin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/terser-webpack-plugin@1.4.5_webpack@4.46.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../terser@4.8.0/node_modules/terser/bin/terser" "$@"
else
  exec node  "$basedir/../../../../../terser@4.8.0/node_modules/terser/bin/terser" "$@"
fi
