#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/vue-demi@0.5.4/node_modules/vue-demi/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/vue-demi@0.5.4/node_modules/vue-demi/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/vue-demi@0.5.4/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/vuepress-theme-reco@1.6.6_vuepress@1.8.2/node_modules/vuepress-theme-reco/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/vuepress-theme-reco@1.6.6_vuepress@1.8.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vue-demi@0.5.4/node_modules/vue-demi/bin/vue-demi-switch.js" "$@"
else
  exec node  "$basedir/../../../../../vue-demi@0.5.4/node_modules/vue-demi/bin/vue-demi-switch.js" "$@"
fi
