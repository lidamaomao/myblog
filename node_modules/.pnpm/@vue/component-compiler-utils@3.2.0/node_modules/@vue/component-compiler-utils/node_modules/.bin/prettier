#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/prettier@1.19.1/node_modules/prettier/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/prettier@1.19.1/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vue/component-compiler-utils@3.2.0/node_modules/@vue/component-compiler-utils/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vue/component-compiler-utils@3.2.0/node_modules/@vue/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vue/component-compiler-utils@3.2.0/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vue/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../prettier@1.19.1/node_modules/prettier/bin-prettier.js" "$@"
else
  exec node  "$basedir/../../../../../../../prettier@1.19.1/node_modules/prettier/bin-prettier.js" "$@"
fi
