#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/protobufjs@5.0.3/node_modules/protobufjs/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/protobufjs@5.0.3/node_modules/protobufjs/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/protobufjs@5.0.3/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/leancloud-realtime@5.0.0-rc.5/node_modules/leancloud-realtime/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/leancloud-realtime@5.0.0-rc.5/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../protobufjs@5.0.3/node_modules/protobufjs/bin/pbjs" "$@"
else
  exec node  "$basedir/../../../../../protobufjs@5.0.3/node_modules/protobufjs/bin/pbjs" "$@"
fi
