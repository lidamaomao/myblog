#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/vue-demi@0.5.4_vue@2.6.12/node_modules/vue-demi/bin/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/vue-demi@0.5.4_vue@2.6.12/node_modules/vue-demi/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/vue-demi@0.5.4_vue@2.6.12/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vuepress-reco/core@1.6.2/node_modules/@vuepress-reco/core/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vuepress-reco/core@1.6.2/node_modules/@vuepress-reco/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vuepress-reco/core@1.6.2/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/@vuepress-reco/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../vue-demi@0.5.4_vue@2.6.12/node_modules/vue-demi/bin/vue-demi-fix.js" "$@"
else
  exec node  "$basedir/../../../../../../../vue-demi@0.5.4_vue@2.6.12/node_modules/vue-demi/bin/vue-demi-fix.js" "$@"
fi
