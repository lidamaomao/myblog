#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/browserslist@4.16.3/node_modules/browserslist/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/browserslist@4.16.3/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/caniuse-api@3.0.0/node_modules/caniuse-api/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/caniuse-api@3.0.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.16.3/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../browserslist@4.16.3/node_modules/browserslist/cli.js" "$@"
fi
