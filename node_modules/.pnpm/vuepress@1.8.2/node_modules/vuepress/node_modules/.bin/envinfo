#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/data/wwwroot/vuepress/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/dist/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/envinfo@7.8.1/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/node_modules:/data/wwwroot/vuepress/node_modules:/data/wwwroot/node_modules:/data/node_modules:/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/vuepress@1.8.2/node_modules/vuepress/node_modules:/data/wwwroot/vuepress/node_modules/.pnpm/vuepress@1.8.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../envinfo@7.8.1/node_modules/envinfo/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../envinfo@7.8.1/node_modules/envinfo/dist/cli.js" "$@"
fi
